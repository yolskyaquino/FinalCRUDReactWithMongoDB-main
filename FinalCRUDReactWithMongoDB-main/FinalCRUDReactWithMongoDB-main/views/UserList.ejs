<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="/main.js"></script>
    <title>Userlist</title>
</head>
<body>
    <table border="1">
        <tr>
            <th>Username</th>
            <th>Password</th>
            <th>Action</th>
        </tr>
        <% users.forEach(user=>{%>
            <tr>
                <td><%= user.username %></td>
                <td><%= user.password %></td>
                <td>
                    <a href="/EditUser/<%= user._id%>">Edit</a>
                    <button onclick="ConfirmDelete('<%= user._id %>')">Delete</button>
                </td>
            </tr>
        <% })  %>
    </table>
</body>
</html>